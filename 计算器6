mainwindow.h

#ifndef MAINWINDOW_H
#define MAINWINDOW_H

#include <QMainWindow>
#include <QtMath>
#include <QStack>

namespace Ui {
class MainWindow;
}

class MainWindow : public QMainWindow
{
    Q_OBJECT

public:
    explicit MainWindow(QWidget *parent = nullptr);
    ~MainWindow();

private:
    double shuzi1;
    double shuzi2;
    double result;
    int jinzhi;
    char zifu2;
    int flag;
    QString xianshi;
    QString biaodashi;

public:
    QStack <int>a;
    QStack <char>b;

public:
    void tojinzhi(int);
    void dataa();
    double jisuan(int,int,char);
    void jisuan1(char);
    int yueshu(int a,int b);
    int beishu(int a,int b);
    double fan(double x);
    bool prio(QStack <char>&q,char x);
    int jisuan2();
    int zhi(int,int);

private slots:
    void on__1_clicked();

    void on__2_clicked();

    void on__3_clicked();

    void on__4_clicked();

    void on__5_clicked();

    void on__6_clicked();

    void on__7_clicked();

    void on__8_clicked();

    void on__9_clicked();

    void on__0_clicked();

    void on_point_clicked();

    void on_leftk_clicked();

    void on_rightk_clicked();

    void on_AC_clicked();

    void on_a_clicked();

    void on_b_clicked();

    void on_c_clicked();

    void on_d_clicked();

    void on_e_clicked();

    void on_delete_2_clicked();

    void on_add_clicked();

    void on_equ_clicked();

    void on_x_clicked();

    void on_f_clicked();

    void on_bin_clicked();

    void on_sub_clicked();

    void on_mul_clicked();

    void on_divide_clicked();

    void on_compare_clicked();

    void on_yueshu_clicked();

    void on_beishu_clicked();

    void on_sin_clicked();

    void on_cos_clicked();

    void on_tan_clicked();

    void on_fanbili_clicked();

    void on_zhishu_clicked();

    void on_lg_clicked();

private:
    Ui::MainWindow *ui;
};

#endif // MAINWINDOW_H


main.cpp


#include "mainwindow.h"
#include <QApplication>

int main(int argc, char *argv[])
{
    QApplication a(argc, argv);
    MainWindow w;
    w.show();

    return a.exec();
}


mainwindow.cpp


#include "mainwindow.h"
#include "ui_mainwindow.h"
#include <QDebug>
#include <QtMath>

MainWindow::MainWindow(QWidget *parent) :
    QMainWindow(parent),
    ui(new Ui::MainWindow)
{
    ui->setupUi(this);
    dataa();


}

void MainWindow::dataa(){
    shuzi1=0;
    shuzi2=0;
    result=0;
    jinzhi=10;
    zifu2='\0';
    xianshi="";
    biaodashi="";
    flag=0;
}

double MainWindow::jisuan(int a,int b,char x)
{
    switch(x)
    {
    case '+': b+=a;
        break;
    case '-': b-=a;
        break;
    case '*': b*=a;
        break;
    case '/':b/=a;
        break;
    }
    return b;
}

void MainWindow::jisuan1(char x)
{   if(x=='s'){
        result=qSin(shuzi2);
        shuzi1=result;
        shuzi2=0;
    }
    if(x=='o'){
        result=qCos(shuzi2);
        shuzi1=result;
        shuzi2=0;
    }
    if(x=='t'){
        result=qTan(shuzi2);
        shuzi1=result;
        shuzi2=0;
    }
    if(x=='l'){
        result=qLn(shuzi1)/qLn(10);

    }
    if(x=='y'){
        int gongyue=yueshu(int(shuzi1),int(shuzi2));
        xianshi+="最大公约数为";
        ui->text->setText(xianshi);
        QString str=QString::number(gongyue);
        xianshi+=str;
        ui->text->setText(xianshi);
    }
    if(x=='^'){
        int x=zhi(int(shuzi1),int(shuzi2));
        QString str=QString::number(x);
        xianshi+=str;
        ui->text->setText(xianshi);
    }
    if(x=='b'){
        int gongbei=beishu(int(shuzi1),int(shuzi2));
        xianshi+="最小公倍数为";
        ui->text->setText(xianshi);
        QString str=QString::number(gongbei);
        xianshi+=str;
        ui->text->setText(xianshi);
    }
    if(x=='f'){
        result=fan(shuzi1);

    }
    if(x=='c'){
        if(shuzi1>shuzi2){
            QString str=QString::number(shuzi1);
            xianshi+=str;
            ui->text->setText(xianshi);
            xianshi+='>';
            ui->text->setText(xianshi);
            str=QString::number(shuzi2);
            xianshi+=str;
            ui->text->setText(xianshi);
        }
        else if (shuzi1==shuzi2) {
            QString str=QString::number(shuzi1);
            xianshi+=str;
            ui->text->setText(xianshi);
            xianshi+='=';
            ui->text->setText(xianshi);
            str=QString::number(shuzi2);
            xianshi+=str;
            ui->text->setText(xianshi);
        }
        else {
            QString str=QString::number(shuzi1);
            xianshi+=str;
            ui->text->setText(xianshi);
            xianshi+='<';
            ui->text->setText(xianshi);
            str=QString::number(shuzi2);
            xianshi+=str;
            ui->text->setText(xianshi);
        }
    }
}

int MainWindow::yueshu(int a, int b)
{
    int r;
    for (r=a%b;r!=0;r=a%b) {
        a=b;
        b=r;
    }
    return b;
}

int MainWindow::beishu(int a, int b)
{
    int gongyue=yueshu(a,b);
    int i;
    for (i=1;;i++) {
        if((gongyue*i)%a==0&&(gongyue*i)%b==0)break;
    }
    int gongbei=gongyue*i;
    return gongbei;
}

double MainWindow::fan(double x)
{
    return 1/x;
}

bool MainWindow::prio(QStack<char> &q, char x)
{
    if (q.empty()||q.top()=='#'||(q.top()=='('&&x!=')')||x=='(')
        return true;
    if (x=='#'||x==')')
        return false;
    if (x == '*' || x == '/' || x == '+' || x == '-')
    {
        if (q.top() == '+' || q.top() == '-') return true;
        return false;
    }
    return true;
}

int MainWindow::jisuan2()
{
    char *s = biaodashi.toLocal8Bit().data();
    int x = 0;
    b.push('#');
    for(int i = 0;i < strlen(s);i++)//警告：好像因为：i是signed，而strlen(s)是unsigned
    {
        if(isdigit(s[i]))//转换个位数为十位数，十位数为百位数，使能够计算多位数
        {
            int Temp = int(s[i] - '0');//强制类型转换
             x = x * 10 + Temp;
            if(!isdigit(s[i+1]))
            {
                a.push(x);
                x = 0;
            }
        }
        else
        {
            if(prio(b,s[i]))
                b.push(s[i]);
            else
            {
                if(b.top() == '(' && s[i] == ')')
                {
                    b.pop();
                    continue;//终止这一次循环，进入下一次循环
                }
                while(!prio(b,s[i]))
                {
                    int m = a.top();
                    a.pop();
                    int n = a.top();
                    a.pop();
                    char top = b.top();
                    b.pop();

                    n =jisuan(m,n,top);

                    a.push(n);

                    if(b.top() == '#')
                        break;
                    if(s[i] == '#')
                        continue;
                    if(b.top() == '(' && s[i] == ')')
                    {
                        b.pop();
                        break;
                    }

                }
                if(s[i] != ')')
                    b.push(s[i]);

            }

        }

    }

    return a.top();
}

int MainWindow::zhi(int a,int b)//指数
{
    int x=1;
    for(int i=1;i<=b;i++){
        x=x*a;
    }
    return x;
}

MainWindow::~MainWindow()
{
    delete ui;
}

void MainWindow::tojinzhi(int n){

    if(flag==0)
    shuzi1=jinzhi*shuzi1+n;
    else
    shuzi2=jinzhi*shuzi2+n;


}

void MainWindow::on__1_clicked()
{

    tojinzhi(1);
    xianshi +="1" ;
    ui->text->setText(xianshi);
}

