{
    ui->text->insert("(");
    xianshi+='(';
    biaodashi+='(';
}

void MainWindow::on_rightk_clicked()
{
    ui->text->insert(")");
    xianshi+=')';
    biaodashi+=QString::number(shuzi1);
    shuzi1=0;
    biaodashi+=')';
}

void MainWindow::on_AC_clicked()
{
    ui->text->clear();
    dataa();
}

void MainWindow::on_add_clicked()
{
    char *s = biaodashi.toLocal8Bit().data();
    if(s[strlen(s)-1]!=')')
    biaodashi+=QString::number(shuzi1);
    shuzi1=0;
    biaodashi+='+';
    jinzhi=10;
    xianshi+='+';
    ui->text->setText(xianshi);

}

void MainWindow::on_sub_clicked()
{
    char *s = biaodashi.toLocal8Bit().data();
    if(s[strlen(s)-1]!=')')
    biaodashi+=QString::number(shuzi1);
    shuzi1=0;
    biaodashi+='-';
    jinzhi=10;
    xianshi +='-';
    ui->text->setText(xianshi);

}

void MainWindow::on_mul_clicked()
{
    char *s = biaodashi.toLocal8Bit().data();
    if(s[strlen(s)-1]!=')')
    biaodashi+=QString::number(shuzi1);
    shuzi1=0;
    biaodashi+='*';
    jinzhi=10;
    xianshi +='*';
    ui->text->setText(xianshi);

}

void MainWindow::on_divide_clicked()
{
    char *s = biaodashi.toLocal8Bit().data();
    if(s[strlen(s)-1]!=')')
    biaodashi+=QString::number(shuzi1);
    shuzi1=0;
    biaodashi+='/';
    jinzhi=10;
    xianshi +='/';
    ui->text->setText(xianshi);
}

void MainWindow::on_equ_clicked()
{
    if(zifu2=='c'||zifu2=='y'||zifu2=='b'||zifu2=='^'){
        xianshi+=' ';
        ui->text->setText(xianshi);
        jisuan1(zifu2);
    }
    else if(zifu2=='s'||zifu2=='o'||zifu2=='t'||zifu2=='f'||zifu2=='l'){
        jisuan1(zifu2);
        xianshi+='=';
        ui->text->setText(xianshi);
        //qDebug()<<result;
        QString str=QString::number(result);
        xianshi+=str;
        ui->text->setText(xianshi);
    }
    else{
        ui->text->insert("=");
        char *s = biaodashi.toLocal8Bit().data();
        if(s[strlen(s)-1]!=')')
        biaodashi+=QString::number(shuzi1);
        qDebug()<<biaodashi;
        biaodashi += '#';
        double x=jisuan2();
        QString str = QString::number(x);
        ui->text->insert(str);
    }
}

void MainWindow::on_compare_clicked()
{
    zifu2='c';
    flag=1;
    xianshi+=" ";
    ui->text->setText(xianshi);
    jisuan1(zifu2);

}

